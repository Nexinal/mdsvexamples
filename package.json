    "postcss": "^8.4.27",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "release-it": "^15.11.0",
    "svelte": "^4.0.0",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.5.23",
